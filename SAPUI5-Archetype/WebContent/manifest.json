{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "com.raprins.archetype",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{application.title}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "com.raprins.archetype.view.PageContainer",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.raprins.archetype.i18n.i18n"
				}
			},
			"mymodel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/model.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "com.raprins.archetype.view",
				"controlId": "mainPageContainer",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				}
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"notFound": {
					"viewName": "NotFound",
					"transition": "show"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/custom.css"
				}
			],
			"js":[
				{
					"uri" : "libraries/lodash.core.js"
				}
			]
		}
	}
}
