<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="com.raprins.music.controller.Home"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Home">
		<content>
			<List id="idListMusic" 
			    items="{
			    	path : 'musics>/results/'
			    }"
			    >
			    <items>
			        <StandardListItem title="{musics>trackName}"/>
			    </items>
			</List>
		</content>
	</Page>
</mvc:View>